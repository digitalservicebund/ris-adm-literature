{
  "name": "ris-adm-vwv-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e:preparation": "playwright test -- e2e/seed-data.ts",
    "test:e2e": "playwright test",
    "test:e2e:debug": "PWDEBUG=1 playwright test --ui",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "oxlint --fix",
    "lint:check": "oxlint",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "audit:licences": "license-checker-rseidelsohn --production --excludePrivatePackages --onlyAllow \"$(. ./joinAllowedLicenses.sh)\" --csv --out ./frontend-licence-report.csv",
    "preinstall": "echo 'Please disable script execution: npm config set ignore-scripts true' && exit 1"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@digitalservicebund/ris-ui": "^3.21.0",
    "@guardian/prosemirror-invisibles": "^3.1.1",
    "@iconify-json/ic": "^1.2.4",
    "@iconify-json/material-symbols": "^1.2.51",
    "@iconify-json/mdi": "^1.2.2",
    "@sentry/vue": "^10.32.1",
    "@tiptap/core": "^3.15.3",
    "@tiptap/extension-blockquote": "^3.11.0",
    "@tiptap/extension-document": "^3.12.1",
    "@tiptap/extension-image": "^3.15.3",
    "@tiptap/extension-list": "^3.15.3",
    "@tiptap/extension-paragraph": "^3.15.3",
    "@tiptap/extension-subscript": "^3.15.3",
    "@tiptap/extension-superscript": "^3.15.3",
    "@tiptap/extension-text-align": "^3.15.0",
    "@tiptap/extension-text-style": "^3.15.3",
    "@tiptap/pm": "^3.6.7",
    "@tiptap/starter-kit": "^3.15.3",
    "@tiptap/vue-3": "^3.15.3",
    "@vueuse/core": "^14.1.0",
    "dayjs": "^1.11.19",
    "keycloak-js": "^26.2.2",
    "pinia": "^3.0.4",
    "primevue": "^4.5.4",
    "prosemirror-model": "^1.25.4",
    "prosemirror-state": "^1.4.4",
    "prosemirror-transform": "^1.10.5",
    "prosemirror-view": "^1.41.5",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.57.0",
    "@sentry/vite-plugin": "^4.6.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node23": "^23.0.4",
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^25.0.6",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vitest/coverage-istanbul": "^4.0.16",
    "@vitest/ui": "^4.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "jsdom": "^27.4.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "postcss": "^8.5.6",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "~4.1.18",
    "typescript": "~5.9.3",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.5",
    "vue-tsc": "^3.2.2"
  },
  "overrides": {
    "vite": {
      "esbuild": "0.25.0"
    }
  },
  "engines": {
    "node": "25.3.0"
  },
  "packageManager": "pnpm@10.24.0"
}
