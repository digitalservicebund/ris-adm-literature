{
  "name": "ris-adm-vwv-frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=23.11.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "PWDEBUG=1 playwright test --ui",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/ e2e/",
    "format:check": "prettier --check src/ e2e/",
    "audit:licences": "license-checker-rseidelsohn --production --excludePrivatePackages --onlyAllow \"$(. ./joinAllowedLicenses.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@digitalservicebund/ris-ui": "^3.4.0",
    "@guardian/prosemirror-invisibles": "^3.1.1",
    "@iconify-json/ic": "^1.2.2",
    "@iconify-json/material-symbols": "^1.2.20",
    "@iconify-json/mdi": "^1.2.2",
    "@sentry/vite-plugin": "^3.3.1",
    "@sentry/vue": "^9.15.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@tiptap/extension-blockquote": "^2.11.7",
    "@tiptap/extension-bold": "^2.11.7",
    "@tiptap/extension-bullet-list": "^2.11.7",
    "@tiptap/extension-color": "^2.11.7",
    "@tiptap/extension-document": "^2.11.7",
    "@tiptap/extension-hard-break": "^2.11.9",
    "@tiptap/extension-history": "^2.11.9",
    "@tiptap/extension-image": "^2.11.7",
    "@tiptap/extension-italic": "^2.11.7",
    "@tiptap/extension-list-item": "^2.11.7",
    "@tiptap/extension-ordered-list": "^2.11.7",
    "@tiptap/extension-paragraph": "^2.11.7",
    "@tiptap/extension-strike": "^2.11.9",
    "@tiptap/extension-subscript": "^2.11.7",
    "@tiptap/extension-superscript": "^2.11.7",
    "@tiptap/extension-text": "^2.11.9",
    "@tiptap/extension-text-align": "^2.11.7",
    "@tiptap/extension-text-style": "^2.11.7",
    "@tiptap/extension-underline": "^2.11.7",
    "@tiptap/pm": "^2.11.7",
    "@tiptap/vue-3": "^2.11.9",
    "@tsconfig/node23": "^23.0.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "immer": "^10.1.1",
    "maska": "^3.1.1",
    "postcss": "^8.5.3",
    "primevue": "^4.3.3",
    "unplugin-icons": "^22.1.0",
    "vite": "^6.3.3",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.10"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.1",
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.3",
    "@vitest/coverage-istanbul": "^3.1.2",
    "@vitest/eslint-plugin": "1.1.43",
    "@vitest/ui": "^3.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.25.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-vue": "^10.0.1",
    "jsdom": "^26.1.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "msw": "^2.7.5",
    "pinia": "^3.0.1",
    "prettier": "^3.5.3",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "~4.1.0",
    "typescript": "~5.8.3",
