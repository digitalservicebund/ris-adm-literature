{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest --reporter=verbose --reporter=json --outputFile=./frontend-unit-test-report.json",
    "test:e2e": "cross-env PLAYWRIGHT_JSON_OUTPUT_NAME=frontend-e2e-test-report.json playwright test --reporter=json",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "coverage": "cross-env TZ=UTC vitest run --coverage",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow \"$(. ./joinAllowedLicenses.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@digitalservice4germany/angie": "^1.2.0",
    "@digitalservice4germany/style-dictionary": "^2.0.0",
    "@digitalservicebund/ris-ui": "^1.7.1",
    "@iconify-json/ic": "^1.2.2",
    "@iconify-json/material-symbols": "^1.2.12",
    "@sentry/vite-plugin": "^2.23.0",
    "@sentry/vue": "^8.48.0",
    "@tsconfig/node23": "^23.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^12.4.0",
    "autoprefixer": "^10.4.20",
    "postcss-import": "^16.1.0",
    "primevue": "^4.2.5",
    "tailwindcss": "^3.4.17",
    "unplugin-icons": "^22.0.0",
    "vite": "^6.0.7",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.7",
    "@playwright/test": "^1.49.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.6",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "eslint": "^9.18.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^26.0.0",
    "license-checker": "^25.0.1",
    "pinia": "^2.3.0",
    "postcss": "^8.5.0",
    "prettier": "^3.3.3",
    "sass": "^1.83.4",
    "typescript": "~5.7.3",
    "vitest": "^2.1.5"
  }
}
