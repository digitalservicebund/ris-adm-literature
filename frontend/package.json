{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --reporter=verbose --reporter=json --outputFile=./frontend-unit-test-report.json",
    "test:e2e": "cross-env PLAYWRIGHT_JSON_OUTPUT_NAME=frontend-e2e-test-report.json playwright test --reporter=json",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "coverage": "TZ=UTC vitest run --coverage",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow \"$(. ./joinAllowedLicenses.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@sentry/vite-plugin": "^2.22.7",
    "@sentry/vue": "^8.48.0",
    "license-checker": "^25.0.1",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@digitalservice4germany/angie": "^1.2.0",
    "@digitalservice4germany/style-dictionary": "^2.0.0",
    "@iconify-json/ic": "^1.2.2",
    "@iconify-json/material-symbols": "^1.2.12",
    "@playwright/test": "^1.49.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/eslint-plugin": "1.1.24",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-scrollbar": "^0.5.1",
    "prettier": "^3.3.3",
    "sass": "^1.83.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "unplugin-icons": "^22.0.0",
    "vite": "^6.0.7",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.2.0"
  }
}
