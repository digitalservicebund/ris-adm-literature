{
  "name": "ris-adm-vwv-frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "24.4.1"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "PWDEBUG=1 playwright test --ui",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/ e2e/",
    "format:check": "prettier --check src/ e2e/",
    "audit:licences": "license-checker-rseidelsohn --production --excludePrivatePackages --onlyAllow \"$(. ./joinAllowedLicenses.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@digitalservicebund/ris-ui": "^3.11.1",
    "@guardian/prosemirror-invisibles": "^3.1.1",
    "@iconify-json/ic": "^1.2.4",
    "@iconify-json/material-symbols": "^1.2.32",
    "@iconify-json/mdi": "^1.2.2",
    "@sentry/vite-plugin": "^4.1.0",
    "@sentry/vue": "^10.5.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tiptap/extension-blockquote": "^3.1.0",
    "@tiptap/extension-document": "^3.1.0",
    "@tiptap/extension-image": "^3.2.0",
    "@tiptap/extension-list": "^3.1.0",
    "@tiptap/extension-paragraph": "^3.1.0",
    "@tiptap/extension-subscript": "^3.1.0",
    "@tiptap/extension-superscript": "^3.1.0",
    "@tiptap/extension-text-align": "^3.1.0",
    "@tiptap/extension-text-style": "^3.2.0",
    "@tiptap/pm": "^3.0.9",
    "@tiptap/starter-kit": "^3.2.0",
    "@tiptap/vue-3": "^3.2.0",
    "@tsconfig/node23": "^23.0.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.6.0",
    "dayjs": "^1.11.13",
    "keycloak-js": "^26.2.0",
    "postcss": "^8.5.6",
    "primevue": "^4.3.7",
    "unplugin-icons": "^22.2.0",
    "vite": "^7.1.2",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.5"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node24": "^24.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.2.1",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/eslint-plugin": "1.3.4",
    "@vitest/ui": "^3.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.33.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-testing-library": "^7.6.6",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "pinia": "^3.0.3",
    "prettier": "^3.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "~4.1.12",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  },
  "overrides": {
    "vite": {
      "esbuild": "0.25.0"
    }
  }
}