                  <ris:sourceDocumentId ris:domainTerm="Dokumentnummer">KSLS013901218</ris:sourceDocumentId>
                  <ris:sourceDocumentRelativePath>/selbstaendige-literatur/KSLS013901218.xml</ris:sourceDocumentRelativePath>
  
